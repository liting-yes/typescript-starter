{
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.fixAll.eslint": "explicit"
  },
  "eslint.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.run": "onSave",
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "typescript",
    "markdown",
    "json",
    "yml",
    "yaml",
    "toml"
  ],
  "cSpell.words": [
    "antfu",
    "bumpp",
    "commitlint",
    "oxlint"
  ]
}
